{
    "name": "python-class-navigator",
    "displayName": "Python Class Navigator",
    "description": "PyCharm-like navigation between parent and child classes and methods for Python",
    "version": "0.0.1",
    "publisher": "your-publisher-name",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/python-class-navigator"
    },
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "keywords": [
        "python",
        "navigation",
        "class",
        "inheritance",
        "method",
        "override"
    ],
    "activationEvents": [
        "onLanguage:python"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "python-class-navigator.navigateToImplementations",
                "title": "Navigate to Child Classes/Method Implementations"
            },
            {
                "command": "python-class-navigator.navigateToSuperclass",
                "title": "Navigate to Parent Class/Method"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "typescript": "^5.0.0"
    }
}
